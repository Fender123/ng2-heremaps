    "@angular/platform-browser-dynamic": "^5.2.4",
    "@types/heremaps": "^3.0.10",
    "codelyzer": "^4.1.0",
    "rollup": "^0.55.5",
    "rxjs": "^5.5.6",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
