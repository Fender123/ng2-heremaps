{
  "name": "ng2-heremaps",
  "version": "0.1.5",
  "description": "Here Maps for Angular 2, 4 , 6...",
  "main": "./dist/bundle/ng2-heremaps.udm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjaric/ng2-heremaps"
  },
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ng2-heremaps.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-heremaps.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "release": "npm run tsc && npm run package && npm publish"
  },
  "author": {
    "name": "Milan Jaric",
    "email": "milan.jaric@gmail.com"
  },
  "license": "MIT",
  "module": "index.js",
  "typings": "./dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "4.3.3",
    "@angular/compiler": "4.3.3",
    "@angular/compiler-cli": "4.3.3",
    "@angular/core": "4.3.3",
    "@angular/platform-browser": "4.3.3",
    "@angular/platform-browser-dynamic": "4.3.3",
    "@types/heremaps": "^3.0.12",
    "codelyzer": "~3.0.1",
    "rollup": "^0.41.6",
    "rxjs": "^5.5.12",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "uglify-js": "^2.8.21",
    "zone.js": "^0.8.26"
  }
